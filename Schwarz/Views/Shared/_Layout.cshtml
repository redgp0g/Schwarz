	<div class="absolute bottom-0 left-0 right-0 px-4 py-3 text-center text-black bg-white border-t-2 border-black h-8 flex justify-center text-xs items-center">
		&copy; 2022 - Criado por Guilherme Gordiano - Inovação e Melhoria Contínua
	</div>
	<script>

		function toggleHidden(id, idbtn) {
			var element = document.getElementById(id);
			var isExpanded = element.getAttribute("aria-expanded") === "true";
			if (isExpanded) {
				element.setAttribute("aria-expanded", "false");
				element.classList.add("hidden");

			} else {
				element.setAttribute("aria-expanded", "true");
				element.classList.remove("hidden");
			}

			if (idbtn != null) {
				document.addEventListener('click', function (event) {
					if (!element.contains(event.target) && event.target.id !== idbtn) {
						element.setAttribute("aria-expanded", "false");
						element.classList.add('hidden');
					}
				});
			}
		}


		setTimeout(function () {
			document.getElementById('notification').classList.add('hidden');
		}, 7000);

	</script>
	<script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.min.js"></script>
	<script src="//cdn.datatables.net/plug-ins/1.13.4/sorting/datetime-moment.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
